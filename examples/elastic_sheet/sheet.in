# Material properties
[Materials]
   [./Membrane]
      [./MembraneThickness]
         value = '0.05'
      [../Density]
         value = '1.0'
      [../StressStrainLaw]
         model = 'incompressible_hyperelasticity'
         strain_energy = 'mooney_rivlin'
         [./MooneyRivlin]
            C1 = '24'
            C2 = '1.5'
[]

[Physics]

   enabled_physics = 'ElasticMembrane'

   [./ElasticMembrane]

      material = 'Membrane'
[]

[BoundaryConditions]
   bc_ids = '1 3 0:2'
   bc_id_name_map = 'Right Left TractionFree'

   [./Left]
      [./Displacement]
         type = 'pinned'
      [../]
   [../]

   [./Right]
      [./Displacement]
         type = 'constant_displacement'
         Ux = '0.1'
      [../]
   [../]

   [./TractionFree]
      [./Displacement]
         type = 'homogeneous_neumann'
      [../]
   [../]
[]

[Variables]
   [./Displacement]
      names = 'Ux Uy'
      fe_family = 'LAGRANGE'
      order = 'FIRST'
[]

[SolverOptions]

   solver_type = 'displacement_continuation'

   [./DisplacementContinuation]
      boundary = '1'
      final_displacement = '8.0'
      n_increments = '50'
[]

# Mesh related options
[Mesh]
   [./Generation]
      dimension = '2'
      element_type = 'QUAD4'
      x_max = '8.0'
      y_max = '8.0'
      n_elems_x = '10'
      n_elems_y = '10'
[]

#Linear and nonlinear solver options
[linear-nonlinear-solver]
max_nonlinear_iterations =  '50'
max_linear_iterations = '2500'

verify_analytic_jacobians = '0.0'

initial_linear_tolerance = '1.0e-10'

# Visualization options
[vis-options]
output_vis = 'true'

vis_output_file_prefix = 'sheet'

output_residual = 'false'

output_format = 'ExodusII xdr'

# Options for print info to the screen
[screen-options]

system_name = 'StretchedElasticSheet'

print_equation_system_info = 'true'
print_mesh_info = 'true'
print_log_info = 'true'
solver_verbose = 'true'
solver_quiet = 'false'

print_element_jacobians = 'false'
print_element_residuals = 'false'
[]
