
#Input file for the immersed boundary benchmark example

# Mesh related options
[Mesh]
  [./Read]
    filename = 'immersed_boundary_benchmark_mesh.exo'
[]


# Properties related to the fluid and solid parameters
[Materials]
  [./Viscosity]
    model = 'constant'
    value = '1.0e-5'
  [../Density]
    value = '1.0'  # Density of the solid
  [../StressStrainLaw]
    model = 'hookes_law'
  [../HookesLaw]
    E = '10000'  # Youngs modulus
    nu = '0.3'   # Poisson's ratio
[]


# Options for time solvers
[SolverOptions]
  theta = '0.5'
  n_timesteps = '10'
  delta_t = '0.1'
[]


[BoundaryConditions]
  bc_ids = '4 2 1:3 6 5:7:8'
  bc_id_name_map = 'Inflow Outflow Walls Cylinder Structure'

  [./Inflow]
    type = 'constant_dirichlet'
    u = '1.0'
    v = '0.0'
  [./Outflow]
    type = 'homogeneous_neumann'
  [./Walls]
    type = 'no_slip'
  [./Cylinder]
    type = 'no_slip'
  [./Structure]
    type = 'no_slip'
[]

#below options are all depracated or just undocumented in master?

# Options for print info to the screen
[screen-options]
print_equation_system_info = 'true'
print_mesh_info = 'true'
print_log_info = 'true'
solver_verbose = 'true'
solver_quiet = 'false'
echo_physics = 'true'
[]

# Options related to all Physics
[Physics]
enabled_physics = 'ImmersedBoundary
                   IncompressibleNavierStokes'

# Options for ImmersedBoundary physics
[./ImmersedBoundary]
## nothing?
[]

[Variables]
   [./Velocity]
      names = 'u v'
      fe_family = 'LAGRANGE'
      order = 'SECOND'
   [../Displacement]
      names = 'x y'
      fe_family = 'LAGRANGE'
      order = 'SECOND'
[]
