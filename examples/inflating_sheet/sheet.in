# Material properties
[Materials]
   [./Membrane]
      [./MembraneThickness]
         value = '0.05'
      [../Density]
         value = '1.0'
      [../StressStrainLaw]
         model = 'incompressible_hyperelasticity'
         strain_energy = 'mooney_rivlin'
         [./MooneyRivlin]
            C1 = '24'
            C2 = '3.0'
[]

[Physics]

   enabled_physics = 'Fluids
                      ImmersedBoundary'
                      
   [./ImmersedBoundary]
      enabled_subdomain = '1'
      solid_mechanics = 'ElasticMembrane'
   [../]
   
   [./ElasticMembrane]
      material = 'Membrane'
   [../]

   [./Fluids]
      enaled_subomdain = '2'
      material = ''
   [../]
[]

[BoundaryConditions]
   bc_ids = '1'
   bc_id_name_map = 'Boundary'

   [./Boundary]
      [./Displacement]
         type = 'pinned'
      [../]
   [../]
[]

[Variables]
   [./Displacement]
      names = 'Ux Uy Uz'
      fe_family = 'LAGRANGE'
      order = 'FIRST'
[]

[SolverOptions]

solver_type = 'pressure_continuation'

[./PressureContinuation]

final_pressure = '3.0'
n_increments = '50'

[]

# Mesh related options
[Mesh]
   [./Read]
      filename = 'sheet_coarse.e'
[]

#Linear and nonlinear solver options
[linear-nonlinear-solver]
relative_residual_tolerance = '1.0e-10'

max_nonlinear_iterations =  '50'
max_linear_iterations = '2500'

verify_analytic_jacobians = '0.0'

initial_linear_tolerance = '1.0e-10'

# Visualization options
[vis-options]
output_vis = 'true'

vis_output_file_prefix = 'sheet'

output_residual = 'false'

output_format = 'ExodusII xdr'

# Options for print info to the screen
[screen-options]

system_name = 'InflatingElasticSheet'

print_equation_system_info = 'true'
print_mesh_info = 'true'
print_log_info = 'true'
solver_verbose = 'true'
solver_quiet = 'false'

print_element_jacobians = 'false'
print_element_residuals = 'false'
[]
