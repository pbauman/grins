# Materials
[Materials]
   [./TestMaterial]
      [./ThermalConductivity]
          model = 'constant'
          value = '1.0'
      [../Density]
         value = '1.0'
      [../SpecificHeat]
         model = 'constant'
         value = '1.0'
[]

# Options related to all Physics
[Physics]

   enabled_physics = 'HeatConduction'

   [./HeatConduction]

      material = 'TestMaterial'
[]

[BoundaryConditions]

   bc_ids = '0 1:2:3'
   bc_id_name_map = 'Dirichlet Neumann'

   [./Dirichlet]
      [./Temperature]
         type = 'parsed_dirichlet'
         T = '0.0'
      [../]
   [../]

   [./Neumann]
      [./Temperature]
         type = 'adiabatic'
      [../]
   [../]

[]


[Variables]
   [./Temperature]
      names = 'T'
      fe_family = 'LAGRANGE'
      order = 'FIRST'
[]

# Mesh related options
[Mesh]
   [./Generation]
      dimension = '2'
      n_elems_x = '3'
      n_elems_y = '3'
      x_min = '0.0'
      x_max = '3.0'
      y_min = '0.0'
      y_max = '3.0'
      element_type = 'QUAD9'
[]

[Strategies]

  [./MeshAdaptivity]
    disable_two_step_refinement = 'true'
  [../]

[]

#Linear and nonlinear solver options
[linear-nonlinear-solver]
   max_nonlinear_iterations =  25
   max_linear_iterations = 2500
   relative_residual_tolerance = '1.0e-14'
   relative_step_tolerance = '1.0e-12'
[]

# Visualization options
[vis-options]
   output_vis = false
   vis_output_file_prefix = 'laplace'
   output_format = 'ExodusII xdr'
[]

# Options for print info to the screen
[screen-options]
   system_name = 'Laplace'
   print_equation_system_info = 'true'
   print_mesh_info = 'true'
   print_log_info = 'true'
   solver_verbose = 'true'
   solver_quiet = 'false'
[]
