
# Material parameters
[Materials]
  [./TestMaterial]
     [./CrossSectionalArea]
        value = '0.1'
     [../Density]
        value = '0.031056'
     [../StressStrainLaw]
        model = 'hookes_law'
        [./HookesLaw]
           nu = '0.45'
           E = '1.0e6'
[]


# Options related to all Physics
[Physics]

   enabled_physics = 'ElasticCable
                      ParsedSourceTerm'

   # Options for Incompressible Navier-Stokes physics
   [./ElasticCable]
      material = 'TestMaterial'

      ic_ids = '0'
      ic_types = 'parsed'
      ic_variables = 'Ux:Uy'
      ic_values = '{-50*sin(x*pi/400)}{-55*sin(x*pi/200)}'
   [../]

   [./ParsedSourceTerm]
      [./Variables]
         names = 'Uy'
         FE_types = 'LAGRANGE'
         FE_orders = 'FIRST'
      [../]

      [./Function]
         value = '32.2*0.1*0.031056'
      [../]
   [../]
[]

[BoundaryConditions]
   bc_ids = '0 1'
   bc_id_name_map = 'LeftWall RightWall'
 
   [./LeftWall]
      [./Displacement]
         type = 'pinned'
      [../]
   [../]
   [./RightWall]
      [./Displacement]
         type = 'roller_x parsed_traction'
         traction = '5.7735 0.0'
[]

[Variables]
   [./Displacement]
      names = 'Ux Uy'
      fe_family = 'LAGRANGE'
      order = 'FIRST'
[]

# Mesh related options
[Mesh]
   [./Generation]
      dimension = '1'
      element_type = 'EDGE2'
      x_min = '0.0'
      x_max = '200.0'
      n_elems_x = '10'
[]

#Linear and nonlinear solver options
[linear-nonlinear-solver]
max_nonlinear_iterations =  '500' 
max_linear_iterations = '2500'

verify_analytic_jacobians = '1.0e-6'

initial_linear_tolerance = '1.0e-10'
minimum_linear_tolerance = '1.0e-11'
relative_step_tolerance = '1.0e-11'

use_numerical_jacobians_only = 'false'
[]

# Visualization options
[vis-options]
output_vis = 'false'

vis_output_file_prefix = 'cable' 

output_residual = 'false' 

output_format = 'ExodusII xdr'
[]

# Options for print info to the screen
[screen-options]

system_name = 'SuspendedCable'

print_equation_system_info = 'true'
print_mesh_info = 'true'
print_log_info = 'true'
solver_verbose = 'true'
solver_quiet = 'false'

print_element_jacobians = 'false'
print_element_residuals = 'false'
[]
